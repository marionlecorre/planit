{% extends "PlanItUserBundle::layout-login.html.twig" %}
{% block fos_user_content %}
<a href="{{ path('PlanItUserBundle_login') }}">
                    Connexion
                </a>
<form action="{{ path('PlanItUserBundle_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
    {{ form_widget(form) }}
    <div>
        <input type="submit" value="{{ 'registration.submit'|trans }}" />
    </div>
</form>
{% endblock fos_user_content %}